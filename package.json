{
  "name": "wap_v3",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "availity-reactstrap-validation": "^1.4.3",
    "axios": "^0.17.1",
    "history": "^4.7.2",
    "husky": "^0.14.3",
    "immutability-helper": "^2.6.4",
    "jquery": "^3.3.1",
    "lint-staged": "^6.0.1",
    "lodash": "^4.17.4",
    "node-sass-chokidar": "^0.0.3",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.10.2",
    "rc-slider": "^8.6.0",
    "react": "^16.2.0",
    "react-circular-progressbar": "^0.8.0",
    "react-datetime": "^2.14.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-scripts": "1.1.0",
    "react-scrollspy": "^3.3.5",
    "react-select": "^1.2.1",
    "react-sortable-hoc": "^0.6.8",
    "reactstrap": "^5.0.0-alpha.4",
    "redux": "^3.7.2",
    "redux-auth-wrapper": "^2.0.2",
    "redux-persist": "^5.5.0",
    "redux-thunk": "^2.2.0",
    "url-search-params": "^0.10.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --no-semi --write",
      "git add"
    ]
  },
  "scripts": {
    "precommit": "lint-staged",
    "build-css": "node-sass-chokidar --include-path ./node_modules src/index.scss -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./node_modules src/index.scss -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
