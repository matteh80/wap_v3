  "private": true,
  "dependencies": {
    "availity-reactstrap-validation": "2.0.2",
    "axios": "0.18.0",
    "faker": "^4.1.0",
    "google-map-react": "^0.34.0",
    "history": "^4.7.2",
    "immutability-helper": "^2.6.4",
    "jquery": "^3.3.1",
    "lodash": "^4.17.4",
    "node-sass-chokidar": "^1.0.1",
    "npm-run-all": "^4.1.2",
    "rc-slider": "^8.6.0",
    "react": "^16.2.0",
    "react-circular-progressbar": "^0.8.0",
    "react-cropper": "^1.0.1",
    "react-dom": "^16.2.0",
    "react-dropzone": "^4.2.9",
    "react-ga": "^2.4.1",
    "react-masonry-component": "^6.0.2",
    "react-notification-system": "^0.2.17",
    "react-notification-system-redux": "^1.2.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-scripts": "1.1.1",
    "react-scrollspy": "^3.3.5",
    "react-select": "^1.2.1",
    "react-sortable-hoc": "^0.6.8",
    "reactstrap": "5.0.0",
    "redux": "^3.7.2",
    "redux-auth-wrapper": "^2.0.2",
    "redux-persist": "^5.5.0",
    "redux-thunk": "^2.2.0",
    "url-search-params": "^0.10.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --no-semi --write",
      "git add"
    ]
  },
  "scripts": {
    "analyze": "source-map-explorer build/static/js/main.*",
    "precommit": "lint-staged",
    "build-css": "node-sass-chokidar --include-path ./node_modules src/index.scss -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./node_modules src/index.scss -o src/ --watch --recursive",
    "start-js": "react-app-rewired start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "husky": "^0.14.3",
    "lint-staged": "7.0.0",
    "prettier": "1.11.1",
    "react-app-rewire-hot-loader": "^1.0.0",
    "react-app-rewired": "^1.5.0",
    "react-hot-loader": "^4.0.0"
  }
}
